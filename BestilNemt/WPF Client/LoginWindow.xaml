<Window x:Name="Login" x:Class="WPF_Client.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Client"
        mc:Ignorable="d"
        Title="LoginWindow" Height="135" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="UsernameField" Margin="86,4,10,0" TextWrapping="Wrap" Text="" Height="23" VerticalAlignment="Top"/>
        <Label x:Name="UsernameLabel" Content="Brugernavn" HorizontalAlignment="Left" Margin="10,1,0,0" VerticalAlignment="Top"/>
        <Label x:Name="PasswordLabel" Content="Kodeord" HorizontalAlignment="Left" Margin="10,32,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.1,0.409"/>
        <PasswordBox x:Name="PasswordField" Height="23" Margin="86,35,10,0" VerticalAlignment="Top" />
        <Button x:Name="LoginConfirm" Content="Login" Margin="207,63,10,10" Click="LoginConfirm_Click"/>
    </Grid>
</Window>
